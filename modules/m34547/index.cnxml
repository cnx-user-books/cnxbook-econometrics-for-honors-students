   <m:mo>∑</m:mo><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
    <m:mtable>
     <m:mtr>
      <m:mtd>
       <m:mrow>
        <m:msubsup>
         <m:mi>σ</m:mi>
         <m:mi>ε</m:mi>
         <m:mn>2</m:mn>
