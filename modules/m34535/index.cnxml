    </m:mrow>
   <m:mo>)</m:mo></m:mrow>
  </m:mrow>
  <m:annotation encoding="MathType-MTEF">MathType@MTEF@5@5@@42BD@</m:annotation>
 </m:semantics>
</m:math>
<!-- MathType@End@5@5@ -->
