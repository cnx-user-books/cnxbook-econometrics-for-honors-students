  <m:annotation encoding="MathType-MTEF">MathType@MTEF@5@5@@67D5@</m:annotation>
 </m:semantics>
</m:math>
<!-- MathType@End@5@5@ --></para><para id="eip-957"><!-- MathType@Translator@5@5@MathML2 (m namespace).tdl@MathML 2.0 (m namespace)@ -->
<m:math display="block">
 <m:semantics>
  <m:mrow>
   <m:msub>
    <m:mi>q</m:mi>
    <m:mi>t</m:mi>
   </m:msub>
   <m:mo>−</m:mo><m:msub>
    <m:mi>α</m:mi>
    <m:mn>1</m:mn>
   </m:msub>
   <m:msub>
    <m:mi>β</m:mi>
    <m:mn>1</m:mn>
   </m:msub>
   <m:msub>
