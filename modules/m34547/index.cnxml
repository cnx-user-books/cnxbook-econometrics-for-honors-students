        </m:mrow>
        
       </m:mstyle>
      </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>+</m:mo><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>1</m:mn><m:mo>−</m:mo><m:msub>
