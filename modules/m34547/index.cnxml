   <m:msup>
    <m:mrow/>
    <m:mo>′</m:mo>
   </m:msup>
   <m:mstyle mathsize="normal" mathvariant="bold"><m:mi>γ</m:mi></m:mstyle><m:mo>.</m:mo>
