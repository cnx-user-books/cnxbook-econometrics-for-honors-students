  <m:annotation encoding="MathType-MTEF">MathType@MTEF@5@5@@39FC@</m:annotation>
 </m:semantics>
</m:math>
<!-- MathType@End@5@5@ -->
 as:</para>
          
          <equation id="eip-80"><!-- MathType@Translator@5@5@MathML2 (m namespace).tdl@MathML 2.0 (m namespace)@ -->
<m:math display="inline">
 <m:semantics>
