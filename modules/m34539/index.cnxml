    <m:mi>x</m:mi>
    <m:mi>i</m:mi>
   </m:msub>
   <m:mo>+</m:mo><m:msub>
    <m:mi>ε</m:mi>
    <m:mi>i</m:mi>
   </m:msub>
   <m:mo>,</m:mo>
