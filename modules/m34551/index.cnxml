    <m:mi>I</m:mi>
    <m:mn>1</m:mn>
    <m:mrow>
     <m:msub>
      <m:mi>β</m:mi>
      <m:mn>1</m:mn>
     </m:msub>
     
    </m:mrow>
   </m:msubsup>
