    </m:mrow>
   </m:mstyle><m:mo>=</m:mo><m:mstyle displaystyle="true">
    <m:munderover>
     <m:mo>∏</m:mo>
     <m:mrow>
      <m:mi>i</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
     <m:mi>n</m:mi>
    </m:munderover>
    <m:mrow>
     <m:mfrac>
      <m:mn>1</m:mn>
      <m:mrow>
       <m:mi>σ</m:mi><m:msqrt>
        <m:mrow>
         <m:mn>2</m:mn><m:mi>π</m:mi>
        </m:mrow>
       </m:msqrt>
       
      </m:mrow>
     </m:mfrac>
     <m:msup>
      <m:mi>e</m:mi>
      <m:mrow>
       <m:mo>−</m:mo><m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:msub>
              <m:mi>y</m:mi>
              <m:mi>i</m:mi>
             </m:msub>
             <m:mo>−</m:mo><m:mi>β</m:mi><m:msub>
              <m:mi>x</m:mi>
              <m:mi>i</m:mi>
             </m:msub>
             
            </m:mrow>
           <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>
         
        </m:mrow>
        <m:mrow>
         <m:mn>2</m:mn><m:msup>
          <m:mi>σ</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         
        </m:mrow>
       </m:mfrac>
       
      </m:mrow>
     </m:msup>
     
    </m:mrow>
   </m:mstyle><m:mo>=</m:mo><m:msup>
    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mfrac>
        <m:mn>1</m:mn>
        <m:mrow>
         <m:mi>σ</m:mi><m:msqrt>
          <m:mrow>
           <m:mn>2</m:mn><m:mi>π</m:mi>
