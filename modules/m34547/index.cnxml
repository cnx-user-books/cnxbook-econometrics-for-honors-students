 </m:semantics>
</m:math>
<!-- MathType@End@5@5@ -->
</equation><para id="id1165166309772">where <!-- MathType@Translator@5@5@MathML2 (m namespace).tdl@MathML 2.0 (m namespace)@ -->
<m:math display="inline">
 <m:semantics>
  <m:mrow>
   <m:mi>ϕ</m:mi><m:mrow><m:mo>(</m:mo>
    <m:mrow>
     <m:msub>
      <m:mstyle mathsize="normal" mathvariant="bold"><m:mi>z</m:mi></m:mstyle>
