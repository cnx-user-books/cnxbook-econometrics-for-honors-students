    <m:mrow>
     <m:mn>2</m:mn><m:mi>π</m:mi>
    </m:mrow>
   </m:msqrt>
   <m:mo>−</m:mo><m:mi>n</m:mi><m:mi>ln</m:mi><m:mover accent="true">
