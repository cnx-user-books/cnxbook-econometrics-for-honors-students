   </m:msub>
   <m:mi>T</m:mi><m:mi>e</m:mi><m:mi>n</m:mi><m:mi>u</m:mi><m:mi>r</m:mi><m:mi>e</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
  </m:mrow>
  <m:annotation encoding="MathType-MTEF">MathType@MTEF@5@5@@4E78@</m:annotation>
 </m:semantics>
</m:math>
<!-- MathType@End@5@5@ -->. or when <!-- MathType@Translator@5@5@MathML2 (m namespace).tdl@MathML 2.0 (m namespace)@ -->
<m:math display="inline">
 <m:semantics>
  <m:mrow>
   <m:mi>T</m:mi><m:mi>e</m:mi><m:mi>n</m:mi><m:mi>u</m:mi><m:mi>r</m:mi><m:mi>e</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
    <m:mrow>
     <m:msub>
      <m:mi>β</m:mi>
