  <m:annotation encoding="MathType-MTEF">MathType@MTEF@5@5@@3DD4@</m:annotation>
 </m:semantics>
</m:math>
<!-- MathType@End@5@5@ -->
 are independent of <!-- MathType@Translator@5@5@MathML2 (m namespace).tdl@MathML 2.0 (m namespace)@ -->
<m:math>
 <m:semantics>
