   </m:msub>
   <m:mo>,</m:mo><m:mtext> </m:mtext><m:mi>i</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mo>…</m:mo><m:mo>,</m:mo><m:mi>N</m:mi><m:mo>,</m:mo><m:mtext> </m:mtext><m:mi>i</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mo>…</m:mo><m:mo>,</m:mo><m:mi>N</m:mi><m:mo>,</m:mo><m:mtext> and </m:mtext><m:mi>t</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mo>…</m:mo><m:mo>,</m:mo><m:mi>T</m:mi><m:mo>.</m:mo>
