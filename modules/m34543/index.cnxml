   </m:msub>
   <m:mo>+</m:mo><m:mstyle displaystyle="true">
    <m:munder>
     <m:mo>∑</m:mo>
     <m:mi>i</m:mi>
    </m:munder>
    <m:mrow>
     <m:msub>
      <m:mi>β</m:mi>
      <m:mi>i</m:mi>
     </m:msub>
