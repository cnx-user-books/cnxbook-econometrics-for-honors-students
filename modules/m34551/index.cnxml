    </m:mrow>
    <m:mrow>
     <m:mo>∂</m:mo><m:mi>T</m:mi><m:mi>e</m:mi><m:mi>n</m:mi><m:mi>u</m:mi><m:mi>r</m:mi><m:mi>e</m:mi>
