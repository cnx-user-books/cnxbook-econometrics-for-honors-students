    </m:msup>
    
   </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>=</m:mo><m:mi>V</m:mi><m:mrow><m:mo>(</m:mo>
    <m:mover accent="true">
     <m:mi>θ</m:mi>
     <m:mo>^</m:mo>
    </m:mover>
    
   <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:msup>
    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mi>B</m:mi><m:mi>i</m:mi><m:mi>a</m:mi><m:mi>s</m:mi><m:mrow><m:mo>[</m:mo> <m:mover accent="true">
        <m:mi>θ</m:mi>
        <m:mo>^</m:mo>
       </m:mover>
        <m:mo>]</m:mo></m:mrow>
      </m:mrow>
     <m:mo>)</m:mo></m:mrow>
    </m:mrow>
    <m:mn>2</m:mn>
   </m:msup>
   <m:mo>.</m:mo>
  </m:mrow>
  <m:annotation encoding="MathType-MTEF">MathType@MTEF@5@5@@56DE@</m:annotation>
 </m:semantics>
</m:math>
<!-- MathType@End@5@5@ -->
 An estimator is asymptotically efficient if  <!-- MathType@Translator@5@5@MathML2 (m namespace).tdl@MathML 2.0 (m namespace)@ -->
<m:math>
 <m:semantics>
  <m:mrow>
   <m:munder>
