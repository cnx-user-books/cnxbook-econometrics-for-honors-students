   <m:msub>
    <m:mi>x</m:mi>
    <m:mi>i</m:mi>
   </m:msub>
   <m:mo>∼</m:mo><m:mi>N</m:mi><m:mrow><m:mo>(</m:mo>
    <m:mrow>
     <m:msub>
      <m:mi>μ</m:mi>
      <m:mi>i</m:mi>
     </m:msub>
     <m:mo>,</m:mo><m:msubsup>
