      </m:mover>
      
      <m:mn>2</m:mn>
     </m:msup>
     
    </m:mrow>
   </m:mfrac>
   <m:mo>=</m:mo><m:mn>0</m:mn>
  </m:mrow>
  <m:annotation encoding="MathType-MTEF">MathType@MTEF@5@5@@5FA3@</m:annotation>
 </m:semantics>
</m:math>
<!-- MathType@End@5@5@ -->
</equation><para id="eip-442">and</para><equation id="eip-281"><!-- MathType@Translator@5@5@MathML2 (m namespace).tdl@MathML 2.0 (m namespace)@ -->
<m:math display="block">
 <m:semantics>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mo>∂</m:mo><m:mi>L</m:mi><m:mrow><m:mo>(</m:mo>
      <m:mrow>
