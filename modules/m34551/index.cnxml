   <m:mi>T</m:mi><m:mi>e</m:mi><m:mi>n</m:mi><m:mi>u</m:mi><m:mi>r</m:mi><m:mi>e</m:mi><m:mo>&gt;</m:mo><m:mn>0.</m:mn>
  </m:mrow>
  <m:annotation encoding="MathType-MTEF">MathType@MTEF@5@5@@45CE@</m:annotation>
