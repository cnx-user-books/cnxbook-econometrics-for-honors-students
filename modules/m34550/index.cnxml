      <m:mi>η</m:mi>
      <m:mi>t</m:mi>
     </m:msub>
     
    </m:mrow>
   <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:msub>
    <m:mi>α</m:mi>
    <m:mn>2</m:mn>
   </m:msub>
   <m:msub>
    <m:mi>I</m:mi>
    <m:mi>t</m:mi>
   </m:msub>
   <m:mo>+</m:mo><m:msub>
    <m:mi>α</m:mi>
