   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>0.</m:mn>
  </m:mrow>
  <m:annotation encoding="MathType-MTEF">MathType@MTEF@5@5@@4582@</m:annotation>
 </m:semantics>
</m:math>
<!-- MathType@End@5@5@ -->
 See any advanced statistics text or <link document="m34535">Statistical terminology</link> for further information on these concepts.</footnote>
</para>
