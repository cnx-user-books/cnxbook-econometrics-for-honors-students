    </m:mrow>
    <m:mrow>
     <m:mi>Φ</m:mi><m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:msub>
        <m:mstyle mathsize="normal" mathvariant="bold"><m:mi>z</m:mi></m:mstyle>
        <m:mi>i</m:mi>
       </m:msub>
