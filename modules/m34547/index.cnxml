    </m:mover>
    
   <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
    <m:mrow>
     <m:msub>
      <m:mi>σ</m:mi>
      <m:mrow>
       <m:mi>ε</m:mi><m:mi>ν</m:mi>
      </m:mrow>
     </m:msub>
     
    </m:mrow>
    <m:mrow>
