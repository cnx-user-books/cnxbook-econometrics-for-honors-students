<!-- MathType@End@5@5@ -->
</equation><para id="id1172359867551">where <!-- MathType@Translator@5@5@MathML2 (m namespace).tdl@MathML 2.0 (m namespace)@ -->
<m:math display="inline">
 <m:semantics>
  <m:mrow>
   <m:msub>
    <m:mi>y</m:mi>
    <m:mrow>
     <m:mi>i</m:mi><m:mi>t</m:mi>
