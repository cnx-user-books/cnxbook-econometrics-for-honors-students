<!-- MathType@End@5@5@ -->
</equation><para id="eip-714">and <!-- MathType@Translator@5@5@MathML2 (m namespace).tdl@MathML 2.0 (m namespace)@ -->
<m:math>
 <m:semantics>
  <m:mrow>
   <m:mrow><m:mo>(</m:mo>
    <m:mrow>
     <m:msub>
      <m:mi>ε</m:mi>
      <m:mi>i</m:mi>
     </m:msub>
