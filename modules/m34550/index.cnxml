    <m:mi>ε</m:mi>
    <m:mi>t</m:mi>
   </m:msub>
   
  </m:mrow>
  <m:annotation encoding="MathType-MTEF">MathType@MTEF@5@5@@61AE@</m:annotation>
 </m:semantics>
</m:math>
<!-- MathType@End@5@5@ -->
</para><para id="eip-76"><!-- MathType@Translator@5@5@MathML2 (m namespace).tdl@MathML 2.0 (m namespace)@ -->
<m:math display="block">
 <m:semantics>
  <m:mrow>
   <m:msub>
    <m:mi>q</m:mi>
    <m:mi>t</m:mi>
   </m:msub>
   <m:mo>=</m:mo><m:msub>
    <m:mi>α</m:mi>
    <m:mn>0</m:mn>
